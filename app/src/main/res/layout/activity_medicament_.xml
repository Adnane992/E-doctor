<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#085394"
    tools:context=".Medicament_Activity">

    <TextView
        android:id="@+id/about_Notif"
        android:layout_width="88dp"
        android:layout_height="39dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/arbutus_slab"
        android:text="About"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2_Medica"
        android:layout_width="375dp"
        android:layout_height="82dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/abril_fatface"
        android:text="List of your medication reminders"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.101" />

    <View
        android:id="@+id/divider_Medica"
        android:layout_width="300dp"
        android:layout_height="3dp"
        android:layout_marginTop="60dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2_Medica" />

</androidx.constraintlayout.widget.ConstraintLayout>